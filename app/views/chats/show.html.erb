<%= form_with model: @chat do |f| %>
    <%= f.text_field :message %>
    <%= f.hidden_field :room_id %>
    <%= f.submit %>
<% end %>
<div class='container'>
  <div class='row'>
    <div class="mx-auto" style="width: 200px;">
      <% @chats.each do |chat| %>
        <p><%= chat.message %></p>
      <% end %>
    </div>
  </div>
</div>

